{
    "quote_latest_stock": {
        "description": "Get latest stock quote",
        "request": {
            "tool_name": "quote.latest",
            "arguments": {
                "symbol": "AAPL",
                "exchange": "NASDAQ",
                "maxAgeSec": 60
            },
            "agent_id": "langchain_agent_001",
            "query_text": "What is the current price of Apple stock?"
        },
        "expected_response": {
            "success": true,
            "data": {
                "symbol": "AAPL",
                "price": 150.50,
                "timestamp": "2025-11-29T12:00:00Z",
                "data_source": "finnhub",
                "cache_hit": false,
                "latency_ms": 245.5,
                "volume": 1500000,
                "high": 152.00,
                "low": 149.00,
                "open": 149.50,
                "previous_close": 148.00
            },
            "cache_hit": false,
            "data_source": "finnhub",
            "latency_ms": 245.5
        }
    },
